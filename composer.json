{
    "name": "nekkoy/gateway-goip",
    "description": "SMS Gateway for GSM GoIP",
    "homepage": "https://github.com/nekkoy/gateway-goip",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "keywords": ["nekkoy", "laravel", "goip", "gsm", "gateway"],
    "authors": [
        {
            "name": "Alexander Dudiak",
            "email": "031991@gmail.com",
            "homepage": "https://github.com/Nekkoy",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nekkoy\\GatewayGoip\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "nekkoy/gateway-abstract": ">=1.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nekkoy\\GatewayGoip\\GoipServiceProvider"
            ],
            "aliases": {
                "GatewayGoip": "Nekkoy\\GatewayGoip\\Facades\\GatewayGoip::class"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
